generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model antri {
  idantri       Int         @id @default(autoincrement())
  idruang       Int?
  idcara_bayar  Int?
  idpasien      Int?
  no            String?     @db.VarChar(10)
  tanggal       DateTime?   @db.Date
  jenis_layanan String?     @db.VarChar(50)
  petugas       Int?
  status        String?     @db.VarChar(40)
  rujukan       String?     @db.VarChar(50)
  nosep         String?     @db.VarChar(100)
  status_bpjs   Int?
  pesan         String?
  log           String?     @db.VarChar(1000)
  waktu_antrian Int?
  pasien        pasien?     @relation(fields: [idpasien], references: [idpasien], onDelete: Restrict, onUpdate: Restrict, map: "FK_RELATIONSHIP_21")
  ruang         ruang?      @relation(fields: [idruang], references: [idruang], onDelete: Restrict, onUpdate: Restrict, map: "FK_RELATIONSHIP_22")
  cara_bayar    cara_bayar? @relation(fields: [idcara_bayar], references: [idcara_bayar], onDelete: Restrict, onUpdate: Restrict, map: "FK_RELATIONSHIP_23")

  @@index([idpasien], map: "FK_RELATIONSHIP_21")
  @@index([idruang], map: "FK_RELATIONSHIP_22")
  @@index([idcara_bayar], map: "FK_RELATIONSHIP_23")
}

model asal_pasien {
  idasal_pasien Int     @id @default(autoincrement())
  keterangan    String? @db.VarChar(50)
}

model biaya_lain {
  idbiaya_lain                            Int      @id @default(autoincrement())
  idinvoice                               Int?
  iddokter                                Int?
  idpetugas_1                             Int?
  idpetugas_2                             Int?
  idpetugas_3                             Int?
  nama_biaya                              String?  @db.VarChar(100)
  tarif                                   Int?
  dokter                                  dokter?  @relation(fields: [iddokter], references: [iddokter], onDelete: Restrict, onUpdate: Restrict, map: "FK_biaya_lain_dokter")
  invoice                                 invoice? @relation(fields: [idinvoice], references: [idinvoice], onDelete: Restrict, onUpdate: Restrict, map: "FK_biaya_lain_invoice")
  petugas_biaya_lain_idpetugas_1Topetugas petugas? @relation("biaya_lain_idpetugas_1Topetugas", fields: [idpetugas_1], references: [idpetugas], onDelete: Restrict, onUpdate: Restrict, map: "FK_biaya_lain_petugas")
  petugas_biaya_lain_idpetugas_2Topetugas petugas? @relation("biaya_lain_idpetugas_2Topetugas", fields: [idpetugas_2], references: [idpetugas], onDelete: Restrict, onUpdate: Restrict, map: "FK_biaya_lain_petugas_2")
  petugas_biaya_lain_idpetugas_3Topetugas petugas? @relation("biaya_lain_idpetugas_3Topetugas", fields: [idpetugas_3], references: [idpetugas], onDelete: Restrict, onUpdate: Restrict, map: "FK_biaya_lain_petugas_3")

  @@index([iddokter], map: "iddokter")
  @@index([idinvoice], map: "idinvoice")
  @@index([idpetugas_1], map: "idpetugas_1")
  @@index([idpetugas_2], map: "idpetugas_2")
  @@index([idpetugas_3], map: "idpetugas_3")
}

model biaya_lain_laboratorium {
  idbiaya_lain_laboratorium                            Int                   @id @default(autoincrement())
  idinvoice_laboratorium                               Int?
  iddokter                                             Int?
  idpetugas_1                                          Int?
  idpetugas_2                                          Int?
  idpetugas_3                                          Int?
  nama_biaya                                           String?               @db.VarChar(100)
  tarif                                                Int?
  dokter                                               dokter?               @relation(fields: [iddokter], references: [iddokter], onDelete: Restrict, onUpdate: Restrict, map: "FK_biaya_lain_laboratorium_dokter")
  invoice_laboratorium                                 invoice_laboratorium? @relation(fields: [idinvoice_laboratorium], references: [idinvoice_laboratorium], onDelete: Restrict, onUpdate: Restrict, map: "FK_biaya_lain_laboratorium_invoice")
  petugas_biaya_lain_laboratorium_idpetugas_1Topetugas petugas?              @relation("biaya_lain_laboratorium_idpetugas_1Topetugas", fields: [idpetugas_1], references: [idpetugas], onDelete: Restrict, onUpdate: Restrict, map: "FK_biaya_lain_laboratorium_petugas")
  petugas_biaya_lain_laboratorium_idpetugas_2Topetugas petugas?              @relation("biaya_lain_laboratorium_idpetugas_2Topetugas", fields: [idpetugas_2], references: [idpetugas], onDelete: Restrict, onUpdate: Restrict, map: "FK_biaya_lain_laboratorium_petugas_2")
  petugas_biaya_lain_laboratorium_idpetugas_3Topetugas petugas?              @relation("biaya_lain_laboratorium_idpetugas_3Topetugas", fields: [idpetugas_3], references: [idpetugas], onDelete: Restrict, onUpdate: Restrict, map: "FK_biaya_lain_laboratorium_petugas_3")

  @@index([iddokter], map: "iddokter")
  @@index([idinvoice_laboratorium], map: "idinvoice_laboratorium")
  @@index([idpetugas_1], map: "idpetugas_1")
  @@index([idpetugas_2], map: "idpetugas_2")
  @@index([idpetugas_3], map: "idpetugas_3")
}

model cara_bayar {
  idcara_bayar Int     @id @default(autoincrement())
  kode         String? @db.VarChar(10)
  arti         String? @db.VarChar(60)
  antri        antri[]
}

model chat {
  id                          Int      @id @default(autoincrement())
  from_chat                   Int      @db.UnsignedInt
  to_chat                     Int      @db.UnsignedInt
  tanggal                     DateTime @db.DateTime(0)
  msg                         String?  @db.Text
  users_chat_from_chatTousers users    @relation("chat_from_chatTousers", fields: [from_chat], references: [id], onUpdate: Restrict, map: "FK_USER_FROM")
  users_chat_to_chatTousers   users    @relation("chat_to_chatTousers", fields: [to_chat], references: [id], onUpdate: Restrict, map: "FK_USER_TO")

  @@index([from_chat], map: "from")
  @@index([to_chat], map: "to")
}

model ci_sessions {
  session_id    String @id @default("0") @db.VarChar(40)
  ip_address    String @default("0") @db.VarChar(45)
  user_agent    String @db.VarChar(120)
  last_activity Int    @default(0) @db.UnsignedInt
  user_data     String @db.Text

  @@index([last_activity], map: "last_activity_idx")
}

model dokter {
  iddokter                Int                       @id @default(autoincrement())
  nama_dokter             String?                   @db.VarChar(100)
  kodedokter              String?                   @db.VarChar(255)
  biaya_lain              biaya_lain[]
  biaya_lain_laboratorium biaya_lain_laboratorium[]
}

model golongan_pembedahan {
  idgolongan_pembedahan Int     @id @default(autoincrement())
  golongan_tindakan     String? @db.VarChar(50)
}

model group_pemeriksaan {
  id                   Int                    @id @default(autoincrement())
  name                 String                 @db.VarChar(255)
  createdAt            DateTime               @db.DateTime(0)
  updatedAt            DateTime               @db.DateTime(0)
  invoice_laboratorium invoice_laboratorium[]
}

model groups {
  id           Int            @id @default(autoincrement()) @db.UnsignedMediumInt
  name         String         @db.VarChar(20)
  description  String         @db.VarChar(100)
  users_groups users_groups[]
}

model hari {
  id   Int    @id @default(autoincrement())
  hari String @db.VarChar(50)
}

model hasil_lab {
  id                  Int      @id @default(autoincrement())
  nilaiNormal         Int?
  invoiceLaboratorium Int?
  hasil               String?  @db.VarChar(255)
  createdAt           DateTime @db.DateTime(0)
  updatedAt           DateTime @db.DateTime(0)

  @@index([invoiceLaboratorium], map: "invoiceLaboratorium")
  @@index([nilaiNormal], map: "nilaiNormal")
}

model inbox {
  inbox_id Int       @id @default(autoincrement())
  subject  String?   @db.VarChar(250)
  message  String?   @db.LongText
  to       Int?
  from     Int?
  status   String?   @default("inbox") @db.VarChar(50)
  is_read  Int?      @default(0)
  created  DateTime? @db.DateTime(0)
  files    String?   @db.VarChar(250)
  to_all   Boolean?
}

model invoice {
  idinvoice            Int          @id @default(autoincrement())
  idpetugas            Int?
  idpetugas_1          Int?
  idpetugas_2          Int?
  idpetugas_3          Int?
  idkunjungan_kasus    Int?
  idpasien             Int?
  idasal_pasien        Int?
  idrujukke            Int?
  iddokter             Int?
  iddokter_onsite      Int?
  iddokter_onsite2     Int?
  iddokter_oncall      Int?
  iddokter_oncall2     Int?
  iddokter_anestesi    Int?
  idkode_penyakit      Int?
  idstatus_keluar      Int?
  idruang              Int?
  idcara_bayar         Int?
  tgl_kunjungan        DateTime?    @db.DateTime(0)
  tgl_bayar            DateTime?    @db.DateTime(0)
  tgl_keluar           DateTime?    @db.DateTime(0)
  tgl_setoran          DateTime?    @db.DateTime(0)
  no_sep               String?      @db.VarChar(20)
  hari_perawatan_ke    String?      @db.VarChar(10)
  kunjungan            String?      @db.VarChar(5)
  tempat_pelayanan     String?      @db.VarChar(100)
  ic                   String?      @db.VarChar(10)
  rujukan_dari         String?      @db.VarChar(50)
  no_khusus            Int?
  anamnesa             String?      @db.VarChar(500)
  anamnesa_2           String?      @db.VarChar(500)
  anamnesa_3           String?      @db.VarChar(500)
  pemeriksaan_fisik_1  String?      @db.VarChar(500)
  pemeriksaan_fisik_2  String?      @db.VarChar(500)
  pemeriksaan_fisik_3  String?      @db.VarChar(500)
  penunjang_diagnosa   String?      @db.VarChar(500)
  diagnosa_kedua       String?      @db.VarChar(500)
  diagnosa_sekunder    String?      @db.VarChar(500)
  pemeriksaan_khusus   String?      @db.VarChar(20)
  advis                String?      @db.VarChar(500)
  tindakan             String?      @db.VarChar(100)
  jenis_tx1            String?      @db.VarChar(50)
  jenis_tx2            String?      @db.VarChar(50)
  jenis_tx3            String?      @db.VarChar(50)
  jenis_tx4            String?      @db.VarChar(50)
  rd1                  String?      @db.VarChar(100)
  rd2                  String?      @db.VarChar(100)
  rd3                  String?      @db.VarChar(100)
  rd4                  String?      @db.VarChar(100)
  rd5                  String?      @db.VarChar(100)
  el_cit               String?      @db.VarChar(100)
  lay_tind_medik       String?      @db.VarChar(30)
  triage               String?      @db.VarChar(15)
  golongan_tindakan    String?      @db.VarChar(50)
  tindakan_gigi_1      String?      @db.VarChar(100)
  tindakan_gigi_2      String?      @db.VarChar(100)
  kamar                String?      @db.VarChar(20)
  buat_puyer           String?      @db.VarChar(500)
  metode_kb            String?      @db.VarChar(20)
  peserta_kb_baru      String?      @db.VarChar(30)
  dirujuk_dengan       String?      @db.VarChar(50)
  keluhan_efek_samping String?      @db.VarChar(30)
  kegiatan_kebid       String?      @db.VarChar(50)
  salin_komplikasi     String?      @db.VarChar(50)
  peri                 String?      @db.VarChar(50)
  peri_lahir_hidup     String?      @db.VarChar(50)
  peri_mati            String?      @db.VarChar(50)
  peri_sebab_mati      String?      @db.VarChar(50)
  pemeriksaan_lain     String?      @db.VarChar(500)
  retribusi_ambulance  Float?       @default(0) @db.Float
  retribusi_oksigen    Float?       @default(0) @db.Float
  status               String?      @db.VarChar(40)
  tanggal_meninggal    DateTime?    @db.DateTime(0)
  uraian_kematian      String?      @db.VarChar(500)
  sebab_kematian       String?      @db.VarChar(100)
  status_obat          Int?
  biaya_lain           biaya_lain[]

  @@index([idruang], map: "FK_RELATIONSHIP_10")
  @@index([idkode_penyakit], map: "FK_RELATIONSHIP_12")
  @@index([iddokter], map: "FK_RELATIONSHIP_14")
  @@index([idpetugas], map: "FK_RELATIONSHIP_15")
  @@index([idkunjungan_kasus], map: "FK_RELATIONSHIP_16")
  @@index([idasal_pasien], map: "FK_RELATIONSHIP_17")
  @@index([idrujukke], map: "FK_RELATIONSHIP_18")
  @@index([iddokter_onsite], map: "FK_RELATIONSHIP_24")
  @@index([iddokter_onsite2], map: "FK_RELATIONSHIP_25")
  @@index([iddokter_oncall], map: "FK_RELATIONSHIP_26")
  @@index([iddokter_oncall2], map: "FK_RELATIONSHIP_27")
  @@index([iddokter_anestesi], map: "FK_RELATIONSHIP_28")
  @@index([idpetugas_1], map: "FK_RELATIONSHIP_37")
  @@index([idpetugas_2], map: "FK_RELATIONSHIP_38")
  @@index([idpetugas_3], map: "FK_RELATIONSHIP_39")
  @@index([idpasien], map: "FK_RELATIONSHIP_6")
  @@index([idstatus_keluar], map: "FK_RELATIONSHIP_8")
  @@index([idcara_bayar], map: "FK_RELATIONSHIP_9")
}

model invoice_laboratorium {
  idinvoice_laboratorium                                    Int                       @id @default(autoincrement())
  idpasien                                                  Int?
  idcara_bayar                                              Int?
  idpetugas                                                 Int?
  iddokter                                                  Int?
  group_pemeriksaan                                         Int?
  tgl_pemeriksaan                                           DateTime?                 @db.DateTime(0)
  tgl_bayar                                                 DateTime?                 @db.DateTime(0)
  tgl_setoran                                               DateTime?                 @db.DateTime(0)
  status_bayar                                              String?                   @db.VarChar(50)
  dari_ruang                                                String?                   @db.VarChar(100)
  rjk_dari                                                  String?                   @db.VarChar(100)
  dokter_pengirim                                           String?                   @db.VarChar(100)
  group_pemeriksaan_group_pemeriksaanToinvoice_laboratorium group_pemeriksaan?        @relation(fields: [group_pemeriksaan], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_GROUP_PEMERIKSAAN")
  biaya_lain_laboratorium                                   biaya_lain_laboratorium[]

  @@unique([idpasien, tgl_pemeriksaan], map: "idpasien_tgl_pemeriksaan")
  @@index([idpasien], map: "FK_RELATIONSHIP_30")
  @@index([idcara_bayar], map: "FK_RELATIONSHIP_31")
  @@index([idpetugas], map: "FK_RELATIONSHIP_66")
  @@index([group_pemeriksaan], map: "group_pemeriksaan")
  @@index([iddokter], map: "iddokter")
}

model invoice_obat {
  idinvoice_obat Int     @id @default(autoincrement())
  idinvoice      Int?
  idobat         Int?
  jumlah_obat    Int?
  aturan_pakai   String? @db.VarChar(500)

  @@index([idinvoice], map: "FK_RELATIONSHIP_19")
  @@index([idobat], map: "FK_RELATIONSHIP_20")
}

model ispa_diare {
  idispa_diare         Int       @id @default(autoincrement())
  idpetugas            Int?
  idruang              Int?
  idstatus_keluar      Int?
  idpasien             Int?
  tgl_kunjungan        DateTime? @db.DateTime(0)
  jenis_pemeriksaan    String?   @db.VarChar(50)
  bb                   Float?    @db.Float
  suhu                 String?   @db.VarChar(10)
  nafas                Float?    @db.Float
  bab                  Float?    @db.Float
  zink                 Boolean?
  dosis_zink           String?   @db.VarChar(20)
  der_dehidrasi        String?   @db.VarChar(20)
  mendapat_oralit      Boolean?
  jumlah_oralit        Float?    @db.Float
  infus_rl             Float?    @db.Float
  tarikan_dinding_dada Boolean?
  cuping_hidung        Boolean?
  antibiotik           Boolean?
  penurun_panas        Boolean?
  ispa                 Boolean?
  pneumonia            Boolean?
  derajat              String?   @db.VarChar(20)
  rujukan_dari         String?   @db.VarChar(50)
  tgl_keluar           DateTime? @db.DateTime(0)

  @@index([idpasien], map: "FK_RELATIONSHIP_62")
  @@index([idpetugas], map: "FK_RELATIONSHIP_63")
  @@index([idruang], map: "FK_RELATIONSHIP_64")
  @@index([idstatus_keluar], map: "FK_RELATIONSHIP_65")
}

model jadwal {
  id        Int      @id @default(autoincrement())
  iddokter  Int?
  idruang   Int?
  kapasitas Int?
  hari      Int?
  jam       String?  @db.VarChar(255)
  createdAt DateTime @db.DateTime(0)
  updatedAt DateTime @db.DateTime(0)

  @@index([iddokter], map: "iddokter")
  @@index([idruang], map: "idruang")
}

model jadwal_operasi {
  id             Int      @id @default(autoincrement())
  idruang        Int
  idpasien       Int
  kodebooking    String   @db.VarChar(10)
  tanggaloperasi DateTime @db.Date
  jenistindakan  String?  @db.VarChar(50)
  terlaksana     Int?     @db.TinyInt
  pasien         pasien   @relation(fields: [idpasien], references: [idpasien], onDelete: NoAction, onUpdate: NoAction, map: "FK__pasien")
  ruang          ruang    @relation(fields: [idruang], references: [idruang], onDelete: NoAction, onUpdate: NoAction, map: "FK__ruang")

  @@index([idpasien], map: "idpasien")
  @@index([idruang], map: "idruang")
}

model jadwal_transaksi {
  id              Int       @id @default(autoincrement())
  tanggal         DateTime? @db.Date
  idjadwal        Int?
  totalantrean    Int?
  sisaantrean     Int?
  antreanpanggil  String?   @db.VarChar(255)
  sisakuotajkn    Int?
  kuotajkn        Int?
  kuotanonjkn     Int?
  sisakuotanonjkn Int?
  isclosed        Boolean?
  createdAt       DateTime  @db.DateTime(0)
  updatedAt       DateTime  @db.DateTime(0)

  @@index([idjadwal], map: "idjadwal")
}

model jenis_pemeriksaan {
  idjenis_pemeriksaan Int     @id @default(autoincrement())
  kode                String? @db.VarChar(10)
  uraian              String? @db.VarChar(100)
  pemeriksaan         String? @db.VarChar(200)
  nilai_normal        String? @db.VarChar(300)
  tarif               Float?  @db.Float
}

model kamar {
  idkamar Int     @id @default(autoincrement())
  ruang   String? @db.VarChar(20)
}

model kategori_obat {
  idkategori_obat Int     @id @default(autoincrement())
  kategori        String? @db.VarChar(40)
}

model kegiatan_kebid_komplikasi {
  idkegiatan_kebid_komplikasi Int     @id @default(autoincrement())
  jenis_kegiatan              String? @db.VarChar(50)
}

model kegiatan_kebidanan {
  idkegiatan_kebidanan Int     @id @default(autoincrement())
  jenis_kegiatan       String? @db.VarChar(50)
}

model kegiatan_peri_kelahiran {
  idkegiatan_peri_kelahiran Int     @id @default(autoincrement())
  jenis_kegiatan            String? @db.VarChar(50)
}

model kegiatan_peri_kematian {
  idkegiatan_peri_kematian Int     @id @default(autoincrement())
  jenis_kegiatan           String? @db.VarChar(50)
}

model kegiatan_peri_sebab_mati {
  idkegiatan_peri_sebab_mati Int     @id @default(autoincrement())
  jenis_kegiatan             String? @db.VarChar(50)
}

model kegiatan_perinatologi {
  idkegiatan_perinatologi Int     @id @default(autoincrement())
  jenis_kegiatan          String? @db.VarChar(50)
}

model kegiatan_radiologi {
  idkegiatan_radiologi Int     @id @default(autoincrement())
  jenis_kegiatan       String? @db.VarChar(50)
}

model keluar_masuk {
  idkeluar_masuk      Int                @id @default(autoincrement())
  idpersediaan_keluar Int?
  idpersediaan_masuk  Int?
  persediaan_keluar   persediaan_keluar? @relation(fields: [idpersediaan_keluar], references: [idpersediaan_keluar], onDelete: NoAction, onUpdate: NoAction, map: "FK_KELUAR_KELUAR")
  persediaan_masuk    persediaan_masuk?  @relation(fields: [idpersediaan_masuk], references: [idpersediaan_masuk], onDelete: NoAction, onUpdate: NoAction, map: "FK_MASUK_MASUK")

  @@index([idpersediaan_keluar], map: "idpersediaan_keluar")
  @@index([idpersediaan_masuk], map: "idpersediaan_masuk")
}

model keluhan_kb {
  idkeluhan_kb         Int     @id @default(autoincrement())
  keluhan_efek_samping String? @db.VarChar(30)
}

model kepatuhan {
  idkepatuhan  Int       @id @default(autoincrement())
  tanggal      DateTime? @db.DateTime(0)
  idruang      Int?
  idpasien     Int?
  gelang       String?   @db.VarChar(50)
  remark       String?   @db.VarChar(250)
  created_date DateTime? @db.DateTime(0)
  created_by   Int       @db.UnsignedInt

  @@index([idpasien], map: "FK_kepatuhan_pasien")
  @@index([idruang], map: "FK_kepatuhan_ruang")
  @@index([created_by], map: "FK_kepatuhan_users")
}

model kepatuhan_obat {
  idkepatuhan_obat Int       @id @default(autoincrement())
  tanggal          DateTime? @db.DateTime(0)
  idobat           Int?
  alert            String?   @db.VarChar(50)
  keterangan       String?   @db.VarChar(250)
  created_date     DateTime? @db.DateTime(0)
  created_by       Int       @db.UnsignedInt

  @@index([idobat], map: "FK_kepatuhan_obat_obat")
  @@index([created_by], map: "FK_kepatuhan_obat_users")
}

model kode_penyakit {
  idkode_penyakit Int     @id @default(autoincrement())
  kode_dx         String? @db.VarChar(10)
  kode_icdx       String? @db.VarChar(10)
  nama_penyakit   String? @db.VarChar(200)
  kelompok        String? @db.VarChar(50)
  kode            String? @db.VarChar(10)
}

model komplain {
  idkomplain     Int              @id @default(autoincrement())
  iduser         Int?             @db.UnsignedInt
  tanggal        DateTime?        @db.DateTime(0)
  judul          String?          @db.VarChar(200)
  isi_komplain   String?          @db.Text
  status         String?          @db.VarChar(50)
  komplain_balas komplain_balas[]

  @@index([iduser], map: "FK__users")
}

model komplain_balas {
  idkomplain_balas Int       @id @default(autoincrement())
  idkomplain       Int?
  iduser           Int?      @db.UnsignedInt
  tanggal          DateTime? @db.DateTime(0)
  isi_balasan      String?   @db.Text
  komplain         komplain? @relation(fields: [idkomplain], references: [idkomplain], onDelete: Cascade, map: "FK__komplain")
